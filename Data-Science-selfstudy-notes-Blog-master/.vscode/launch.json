    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Blog on Chrome",
            "request": "launch",
            "type": "chrome",
            "url": "http://localhost:4000/Data-Science-selfstudy-notes-Blog/",
            "webRoot": "${workspaceFolder}"
        },
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true
        },

    ]
}